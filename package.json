{
  "name": "onclass-newssite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://NonakaMibuki.github.io/NewsSite/",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "rm": "rm -rf docs",
